{
    "skip_files": [
      "lib/buff_ex/config.ex",
      "lib/buff_ex/application.ex",
      "test/support/*",
      "lib/buff_ex_web.ex",
      "lib/buff_ex_web/router.ex",
      "ib/buff_ex_web/views/error_helpers.ex", 
      "lib/buff_ex_web/telemetry.ex"

    ],
  
    "terminal_options": {
